    "homepage": "https://github.com/jenssegers/date",
    "license" : "MIT",
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        },
        {
            "name": "Nguyen Hai Quang",
            "email": "haiquang9994@gmail.com"
        }
    ],
    "require": {
